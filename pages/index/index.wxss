.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index_swiper swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
}

.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}

.index_cate navigator image {
  width: 100%;
}

.index_floor .floor_group .floor_title image {
  display: block;
  width: 100%;
}

.index_floor .floor_group .floor_list {
  padding: 0 10rpx 10rpx;
  display: grid;
  grid-template-columns: repeat(3, calc(710rpx/3));
  grid-template-rows: repeat(2, calc(710rpx/3 * 386 / 232 /2));
  grid-row-gap: 10rpx;
  grid-column-gap: 10rpx;
}

.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}

.index_floor .floor_group .floor_list navigator:nth-child(1) {
  grid-row: 1 / 3;
}
